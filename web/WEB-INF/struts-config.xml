	    path="/buku/form" 
	    scope="request"  
	    validate="true"
	    type="com.ams.action.BukuAction">
	    <forward  name="success"  path="/jsp/buku_form.jsp"  />
	    </action>
	    
	    <action
		input="/jsp/buku_form.jsp"  
	    name="BukuForm"
	    parameter="SAVE"     
	    path="/buku/save" 
	    scope="request"  
	    validate="true"
	    type="com.ams.action.BukuAction">
	    <forward  name="success"  path="/buku/list.do"  />
	    </action>
	    
	    <action
		input="/jsp/buku_list.jsp"  
	    name="BukuForm"
	    parameter="DETAIL"     
	    path="/buku/detail" 
	    scope="request"  
	    validate="true"
	    type="com.ams.action.BukuAction">
	    <forward  name="success"  path="/jsp/buku_detail.jsp"  />
