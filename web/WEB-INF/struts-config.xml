	    <action
		input="/jsp/buku_list.jsp"  
	    name="BukuForm"
	    parameter="DELETE"     
	    path="/buku/delete" 
	    scope="request"  
	    validate="true"
	    type="com.ams.action.BukuAction">
	    <forward  name="success"  path="/buku/list.do"  />
	    </action>
	    
	    <action
        	attribute="BukuForm"
            name="BukuForm"
            parameter="POPUPBUKU"
            path="/buku/popup"
            type="com.ams.action.BukuAction"
            scope="request"
            validate="false">
            <forward name="success" path="/jsp/buku_popup.jsp" />
        </action>
        
	    <!-- user action-->
	    
	    <action
		input="/jsp/user_list.jsp"  
	    name="UserForm"
	    parameter="PARTIALLIST"     
	    path="/user/list" 
	    scope="request"  
	    validate="true"
	    type="com.ams.action.UserAction">
	    <forward  name="success"  path="/jsp/user_list.jsp"  />
	    </action>
	    
	    <action
		input="/jsp/user_list.jsp"  
	    name="UserForm"
	    parameter="FORM"     
	    path="/user/form" 
	    scope="request"  
	    validate="true"
	    type="com.ams.action.UserAction">
	    <forward  name="success"  path="/jsp/user_form.jsp"  />
	    </action>
	    
	    <action
		input="/jsp/user_form.jsp"  
	    name="UserForm"
	    parameter="SAVE"     
	    path="/user/save" 
	    scope="request"  
	    validate="true"
	    type="com.ams.action.UserAction">
	    <forward  name="success"  path="/user/list.do"  />
	    </action>
	    
	    <action
		input="/jsp/user_list.jsp"  
	    name="UserForm"
	    parameter="DETAIL"     
	    path="/user/detail" 
	    scope="request"  
	    validate="true"
	    type="com.ams.action.UserAction">
	    <forward  name="success"  path="/jsp/user_detail.jsp"  />
	    </action>
	    
	    <action
		input="/jsp/user_list.jsp"  
	    name="UserForm"
	    parameter="DELETE"     
