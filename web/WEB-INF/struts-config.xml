	    </action>
	    
	    <action
		input="/jsp/kategori_list.jsp"  
	    name="KategoriBukuForm"
	    parameter="DELETE"     
	    path="/kategori/delete" 
	    scope="request"  
	    validate="true"
	    type="com.ams.action.KategoriBukuAction">
	    <forward  name="success"  path="/kategori/list.do"  />
	    </action>
    
    <!-- buku action-->
	    
	    <action
		input="/jsp/buku_list.jsp"  
	    name="BukuForm"
	    parameter="PARTIALLIST"     
	    path="/buku/list" 
	    scope="request"  
	    validate="true"
	    type="com.ams.action.BukuAction">
	    <forward  name="success"  path="/jsp/buku_list.jsp"  />
