	    validate="true"
	    type="com.ams.action.UserAction">
	    <forward  name="success"  path="/user/list.do"  />
	    </action>
	    
	<!-- Sewa action-->
        <action
        attribute="SewaForm"
		input="/jsp/sewa_list.jsp"  
	    name="SewaForm"
	    parameter="PARTIALLIST"     
	    path="/sewa/list" 
	    scope="request"  
	    validate="false"
	    type="com.ams.action.SewaAction">
	    <forward  name="success"  path="/jsp/sewa_list.jsp"  />
	    </action>
	    
	    <action
		input="/jsp/sewa_list.jsp"  
	    name="SewaForm"
	    parameter="FORM"     
	    path="/sewa/form" 
	    scope="request"  
	    validate="true"
	    type="com.ams.action.SewaAction">
	    <forward  name="form"  path="/jsp/sewa_form.jsp"  />
	    </action>
	    
	    <action
		input="/jsp/sewa_form.jsp"  
	    name="SewaForm"
	    parameter="SAVE"     
	    path="/sewa/save" 
	    scope="request"  
	    validate="false"
	    type="com.ams.action.SewaAction">
	    <forward name="list_redir" path="/sewa/list.do" redirect="true" />
       	<forward name="form" path="/sewa/form.do" redirect="true" />
	    </action>
	    
	    <action
		input="/jsp/sewa_list.jsp"  
	    name="SewaForm"
	    parameter="DETAIL"     
	    path="/sewa/detail" 
	    scope="request"  
	    validate="true"
	    type="com.ams.action.SewaAction">
	    <forward  name="success"  path="/jsp/sewa_detail.jsp"  />
	    </action>
	    
	    <action
		input="/jsp/sewa_list.jsp"  
	    name="SewaForm"
	    parameter="DELETE"     
	    path="/sewa/delete" 
	    scope="request"  
	    validate="true"
	    type="com.ams.action.SewaAction">
	    <forward  name="success"  path="/sewa/list.do" redirect="true"  />
	    </action>
	    
	    <!-- Login -->
	    <action 
	    name="LoginForm"
	    parameter="PERFORMLOGIN"     
	    path="/login" 
	    scope="request"  
	    validate="true"
	    type="com.ams.action.LoginAction">
	    <forward  name="success"  path="/index.jsp"  />
	    <forward name="failed" path="/Login.jsp" />
	    </action>
	    
	</action-mappings>
	
	<message-resources parameter="resource.ApplicationResources" />
     <plug-in className="com.ams.mpe.comon.HibernatePlugIn">
		<!-- 'path-to-config-file' is relative to the root of the classpath.  It MUST start with a '/'. The default is "/hibernate.cfg.xml" -->
		<set-property property="configFilePath1" value="/hibernate.cfg.xml" />
	</plug-in>
	<plug-in className="com.ams.mpe.comon.HibernateUtil" />
 
</struts-config>

