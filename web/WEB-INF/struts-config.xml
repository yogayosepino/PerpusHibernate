<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_1.dtd">

<struts-config>

    <form-beans>   
    
      <!-- Anggota -->
  <form-bean name="AnggotaForm" type="com.ams.form.AnggotaForm">
      <form-property name="id" type="java.lang.String" />
      <form-property name="kode" type="java.lang.String" />
      <form-property name="nama" type="java.lang.String" />
      <form-property name="tglGabung" type="java.lang.String" />
      <form-property name="fromDate" type="java.lang.String" />
      <form-property name="toDate" type="java.lang.String" />
      <form-property name="start" type="java.lang.String" />
      <form-property name="count" type="java.lang.String" />
      <form-property name="total" type="java.lang.String" />
      <form-property name="page" type="java.lang.String" />
      <form-property name="subaction" type="java.lang.String" />
      <form-property name="ascDesc" type="java.lang.String" />
      <form-property name="orderBy" type="java.lang.String" />
      <form-property name="PAGER" type="java.lang.String" />
      <form-property name="PAGERITEM" type="java.lang.String" />
      <form-property name="gotoPage" type="java.lang.String" />
      <form-property name="gotoPage2" type="java.lang.String" />
   </form-bean>  
   
     <!-- KategoriBuku -->
  <form-bean name="KategoriBukuForm" type="com.ams.form.KategoriBukuForm">
      <form-property name="id" type="java.lang.String" />
      <form-property name="nama" type="java.lang.String" />
      <form-property name="start" type="java.lang.String" />
      <form-property name="count" type="java.lang.String" />
      <form-property name="total" type="java.lang.String" />
      <form-property name="page" type="java.lang.String" />
      <form-property name="subaction" type="java.lang.String" />
      <form-property name="ascDesc" type="java.lang.String" />
      <form-property name="orderBy" type="java.lang.String" />
      <form-property name="PAGER" type="java.lang.String" />
      <form-property name="PAGERITEM" type="java.lang.String" />
      <form-property name="gotoPage" type="java.lang.String" />
      <form-property name="gotoPage2" type="java.lang.String" />
      <form-property name="idKategori" type="java.lang.String" />
   </form-bean>  
   
 <!-- Buku -->
  <form-bean name="BukuForm" type="com.ams.form.BukuForm">
     <form-property name="id" type="java.lang.String" />
      <form-property name="kodeBuku" type="java.lang.String" />
      <form-property name="namaBuku" type="java.lang.String" />
      <form-property name="idKategori" type="java.lang.String" />
      <form-property name="namaKategori" type="java.lang.String" />
      <form-property name="jumlahBuku" type="java.lang.String" />
      <form-property name="start" type="java.lang.String" />
      <form-property name="count" type="java.lang.String" />
      <form-property name="total" type="java.lang.String" />
      <form-property name="page" type="java.lang.String" />
      <form-property name="subaction" type="java.lang.String" />
      <form-property name="ascDesc" type="java.lang.String" />
      <form-property name="orderBy" type="java.lang.String" />
      <form-property name="PAGER" type="java.lang.String" />
      <form-property name="PAGERITEM" type="java.lang.String" />
      <form-property name="gotoPage" type="java.lang.String" />
      <form-property name="gotoPage2" type="java.lang.String" />
      
   </form-bean>  
   
    <!-- User -->
  <form-bean name="UserForm" type="com.ams.form.UserForm">
      <form-property name="id" type="java.lang.String" />
      <form-property name="username" type="java.lang.String" />
      <form-property name="password" type="java.lang.String" />
      <form-property name="start" type="java.lang.String" />
      <form-property name="count" type="java.lang.String" />
      <form-property name="total" type="java.lang.String" />
      <form-property name="page" type="java.lang.String" />
      <form-property name="subaction" type="java.lang.String" />
      <form-property name="ascDesc" type="java.lang.String" />
      <form-property name="orderBy" type="java.lang.String" />
      <form-property name="PAGER" type="java.lang.String" />
      <form-property name="PAGERITEM" type="java.lang.String" />
      <form-property name="gotoPage" type="java.lang.String" />
      <form-property name="gotoPage2" type="java.lang.String" />
   </form-bean> 
   
   <!-- Sewa -->
   <form-bean name="SewaForm" type="com.ams.form.SewaForm">
      <form-property name="id" type="java.lang.String"/>
	  <form-property name="idAnggota" type="java.lang.String" />
	  <form-property name="idSewa" type="java.lang.String" />
	  <form-property name="namaAnggota" type="java.lang.String" />
	  <form-property name="tglGabung" type="java.lang.String" />
	  <form-property name="idDetailSewa" type="java.lang.String" />
	  <form-property name="tglSewa" type="java.lang.String" />
      <form-property name="tglKembali" type="java.lang.String" />
      <form-property name="idBuku" type="java.lang.String" />
      <form-property name="judulBuku" type="java.lang.String" />
      <form-property name="jumlahSewa" type="java.lang.String" />
      <form-property name="start" type="java.lang.String" />
      <form-property name="count" type="java.lang.String" />
      <form-property name="total" type="java.lang.String" />
      <form-property name="page" type="java.lang.String" />
      <form-property name="subaction" type="java.lang.String" />
      <form-property name="reportType" type="java.lang.String" />
      <form-property name="ascDesc" type="java.lang.String" />
      <form-property name="orderBy" type="java.lang.String" />
      <form-property name="PAGER" type="java.lang.String" />
      <form-property name="PAGERITEM" type="java.lang.String" />
      <form-property name="gotoPage" type="java.lang.String" />
      <form-property name="gotoPage2" type="java.lang.String" />
   </form-bean>
   
   <!-- Login -->
   <form-bean name="LoginForm" type="com.ams.form.LoginForm">
		<form-property name="username" type="java.lang.String" />
    	<form-property name="password" type="java.lang.String" />
   </form-bean>
        
 </form-beans>
 
 <global-forwards></global-forwards>
 
	<action-mappings>
      
    <!-- anggota action-->
    
	    <action
		input="/jsp/anggota_list.jsp"  
	    name="AnggotaForm"
	    parameter="PARTIALLIST"     
	    path="/anggota/list" 
	    scope="request"  
	    validate="true"
	    type="com.ams.action.AnggotaAction">
	    <forward  name="success"  path="/jsp/anggota_list.jsp"  />
	    </action>
	    
	    <action
		input="/jsp/anggota_list.jsp"  
	    name="AnggotaForm"
	    parameter="FORM"     
	    path="/anggota/form" 
	    scope="request"  
	    validate="true"
	    type="com.ams.action.AnggotaAction">
	    <forward  name="success"  path="/jsp/anggota_form.jsp"  />
	    </action>
	    
	    <action
		input="/jsp/anggota_form.jsp"  
	    name="AnggotaForm"
	    parameter="SAVE"     
	    path="/anggota/save" 
	    scope="request"  
	    validate="true"
	    type="com.ams.action.AnggotaAction">
	    <forward  name="success"  path="/anggota/list.do"  />
	    </action>
	    
	    <action
		input="/jsp/anggota_list.jsp"  
	    name="AnggotaForm"
	    parameter="DETAIL"     
	    path="/anggota/detail" 
	    scope="request"  
	    validate="true"
	    type="com.ams.action.AnggotaAction">
	    <forward  name="success"  path="/jsp/anggota_detail.jsp"  />
	    </action>
	    
	    <action
		input="/jsp/anggota_list.jsp"  
	    name="AnggotaForm"
	    parameter="DELETE"     
	    path="/anggota/delete" 
	    scope="request"  
	    validate="true"
	    type="com.ams.action.AnggotaAction">
	    <forward  name="success"  path="/anggota/list.do"  />
	    </action>
	    
	    <action
        	attribute="AnggotaForm"
            name="AnggotaForm"
            parameter="POPUPANGGOTA"
            path="/anggota/popup"
            type="com.ams.action.AnggotaAction"
            scope="request"
            validate="false">
            <forward name="success" path="/jsp/anggota_popup.jsp" />
        </action>
	  
	  
 <!-- kategori buku action-->
	    
	    <action
		input="/jsp/kategori_list.jsp"  
	    name="KategoriBukuForm"
	    parameter="PARTIALLIST"     
	    path="/kategori/list" 
	    scope="request"  
	    validate="true"
	    type="com.ams.action.KategoriBukuAction">
	    <forward  name="success"  path="/jsp/kategori_list.jsp"  />
	    </action>
	    
	    <action
		input="/jsp/kategori_list.jsp"  
	    name="KategoriBukuForm"
	    parameter="FORM"     
	    path="/kategori/form" 
	    scope="request"  
	    validate="true"
	    type="com.ams.action.KategoriBukuAction">
	    <forward  name="success"  path="/jsp/kategori_form.jsp"  />
	    </action>
	    
	    <action
		input="/jsp/kategori_form.jsp"  
